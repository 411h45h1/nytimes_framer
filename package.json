  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^9.2.1",
    "electron-builder": "^22.8.0",
    "enzyme": "^3.11.0"
  },
  "build": {
    "productName": "The New York Times Snapshot",
    "copyright": "Copyright  Â© 2020 ",
    "mac": {
      "icon": "./public/favicon.ico"
    },
    "win": {
      "icon": "./public/favicon.ico"
    },
    "files": [
      "./build/**/*",
      "./dist/**/*",
      "./node_modules/**/*",
      "./public/**/*",
      "*.js"
